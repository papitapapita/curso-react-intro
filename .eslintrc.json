{
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "plugin:jest/recommended"
    ],
    "rules": {
      "no-console": "warn",
      "prettier/prettier": ["error"]
    },
    "plugins": ["jest", "react"],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  